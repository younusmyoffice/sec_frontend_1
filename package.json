  "scripts": {
    "dev": "webpack-dev-server --mode development --config webconfig/webpack.dev.config.js --hot  --env PLATFORM=local  --env VERSION=dev --progress",
    "prebuild:localtest": "REACT_APP_ENV=local webpack --mode development --config webconfig/webpack.dev.config.js  --env PLATFORM=local  --env VERSION=local --progress",
    "prebuild:dev": "node ./update-build.js && webpack --mode production --config webconfig/webpack.prod.config.js  --env PLATFORM=dev  --env VERSION=dev --progress",
    "prebuild:stage": "node ./update-build.js && webpack --mode production --config webconfig/webpack.prod.config.js  --env PLATFORM=stage  --env VERSION=stage --progress",
    "prebuild:prod": "node ./update-build.js && webpack --mode production --config webconfig/webpack.prod.config.js  --env PLATFORM=prod  --env VERSION=prod --progress",
    "build": "yarn prebuild:dev",
    "test": "jest --config=./webconfig/jest.config.js",
    "test:watch": "jest --config=./webconfig/jest.config.js --watch",
    "test:verbose": "jest --config=./webconfig/jest.config.js --verbose",
    "test:coverage": "jest --config=./webconfig/jest.config.js --coverage",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "lint:updated": "pipe-git-updated --ext=js -- eslint",
    "prepare": "husky install",
    "prepare-release": "standard-version && prettier --write CHANGELOG.md",
    "prettier": "prettier --check .",
    "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,scss,html,js,json,md,yml}\"",
    "prettier-check:updated": "pipe-git-updated --ext=css --ext=scss --ext=html --ext=js --ext=json --ext=md --ext=yml --base=main -- prettier -c",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,scss,html,js,json,md,yml}\"",
    "prettify:updated": "pipe-git-updated --ext=css --ext=scss --ext=html --ext=js --ext=json --ext=md --ext=yml --base=main -- prettier --write"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    },
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "refactor",
        "section": "Maintenance Improvements"
      }
    ]
  },
